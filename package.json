{
  "name": "backend",
  "private": "true",
  "type": "module",
  "engines": {
    "node": "20.x.x",
    "npm": "10.x.x"
  },
  "version": "0.0.1",
  "scripts": {
    "generate": "npx prisma generate",
    "lint:editor": "editorconfig-checker",
    "lint:fs": "ls-lint",
    "build": "npm run generate && npx tsc && tsc-alias -p tsconfig.json && npm run prepare-build",
    "start:dev": "nodemon --exec \"node --loader ts-paths-esm-loader --no-warnings\" src/index.ts",
    "start": "npm run generate && npm run build && node build/index.js",
    "start:vercel": "vercel dev",
    "prepare-build": "sh ./prepare-build.sh"
  },
  "author": "Denis Redkin",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.451.0",
    "@aws-sdk/s3-request-presigner": "^3.454.0",
    "@fastify/auth": "^4.4.0",
    "@fastify/cors": "^8.4.1",
    "@fastify/multipart": "^8.0.0",
    "@fastify/static": "^6.12.0",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^1.10.1",
    "@prisma/client": "^5.6.0",
    "@types/convict": "^6.1.6",
    "bcrypt": "^5.1.1",
    "convict": "^6.2.4",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "fastify-plugin": "^4.5.1",
    "fastify-raw-body": "^4.3.0",
    "filesize": "^10.1.0",
    "handlebars": "^4.7.8",
    "joi": "^17.11.0",
    "jose": "^5.1.1",
    "mime-types": "^2.1.35",
    "nodemailer": "^6.9.7",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "react-dropzone": "^14.2.3",
    "swagger-jsdoc": "^6.2.8",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.2",
    "@commitlint/config-conventional": "^18.4.2",
    "@ls-lint/ls-lint": "^2.2.2",
    "@mermaid-js/mermaid-cli": "^10.6.1",
    "@types/bcrypt": "^5.0.2",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.9.0",
    "@types/nodemailer": "^6.4.14",
    "@types/react-dropzone": "^5.1.0",
    "@types/swagger-jsdoc": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "commitlint": "^18.4.2",
    "editorconfig-checker": "^5.1.1",
    "eslint": "^8.54.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "lint-staged": "^15.1.0",
    "nodemon": "^3.0.1",
    "prettier": "3.1.0",
    "prisma": "^5.6.0",
    "prisma-erd-generator": "^1.11.2",
    "simple-git-hooks": "^2.9.0",
    "ts-paths-esm-loader": "^1.4.3",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx commitlint --edit $1"
  }
}
